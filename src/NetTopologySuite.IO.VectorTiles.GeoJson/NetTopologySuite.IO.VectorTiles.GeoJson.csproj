<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="../../NetTopologySuite.IO.VectorTiles.Common.props" />

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>8</LangVersion>
        <Nullable>enable</Nullable>
        <Title>Cm.NetTopologySuite.IO.VectorTiles.GeoJson</Title>
		<Description>Provides functionality to write GeoJSON tiles</Description>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageId>Cm.$(AssemblyName)</PackageId>
		<Version>1.2301</Version>
		<Authors>Copycd</Authors>
		<Product>Cm.$(AssemblyName)</Product>
		<AssemblyName>Cm.$(MSBuildProjectName)</AssemblyName>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\NetTopologySuite.IO.VectorTiles\NetTopologySuite.IO.VectorTiles.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="NetTopologySuite.IO.GeoJson" Version="3.0.0" />
